{
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.words": [
    "adaptee",
    "associatedtype",
    "creational",
    "csstools",
    "datetime",
    "giscus",
    "isset",
    "jinyongp",
    "lastmod",
    "markdownify",
    "naver",
    "nbcamp",
    "objc",
    "outfile",
    "photoswipe",
    "Pinterest",
    "pswp",
    "sfeed",
    "strikethrough",
    "struct",
    "tailwindcss",
    "typealias",
    "uikit"
  ],
  "css.validate": false,
  "editor.formatOnSave": true,
  "files.associations": {
    "*.css": "postcss",
    "*.svg": "html"
  },
  "json.schemas": [
    {
      "fileMatch": ["frontmatter.json"],
      "url": "https://frontmatter.codes/frontmatter.schema.json"
    },
    {
      "fileMatch": ["manifest.webmanifest"],
      "url": "https://json.schemastore.org/web-manifest"
    }
  ]
}
