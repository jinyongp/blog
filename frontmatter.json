{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.content.pageFolders": [
    {
      "title": "Blog",
      "path": "[[workspace]]/content/blog"
    },
    {
      "title": "Study",
      "path": "[[workspace]]/content/study"
    },
    {
      "title": "Projects",
      "path": "[[workspace]]/content/projects"
    }
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.publicFolder": "static",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.id": "hugo",
  "frontMatter.framework.startCommand": "hugo server -D",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "datetime"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        }
      ]
    }
  ]
}