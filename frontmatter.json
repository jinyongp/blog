{
  "frontMatter.content.pageFolders": [
    {
      "title": "Posts",
      "path": "[[workspace]]/content/posts"
    },
    {
      "title": "Series",
      "path": "[[workspace]]/content/series"
    },
    {
      "title": "Learn",
      "path": "[[workspace]]/content/learn"
    }
  ],
  "frontMatter.content.autoUpdateDate": true,
  "frontMatter.content.publicFolder": "static",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.framework.id": "hugo",
  "frontMatter.framework.startCommand": "hugo server -D",
  "frontMatter.preview.host": "http://localhost:1313",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string",
          "default": "jinyongp"
        },
        {
          "title": "repo",
          "name": "repo",
          "type": "string",
          "default": null
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "draft",
          "name": "draft",
          "type": "boolean",
          "default": true
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "list"
        },
        {
          "title": "series",
          "name": "series",
          "type": "fields",
          "fields": [
            {
              "title": "list",
              "name": "list",
              "type": "string",
              "default": null
            },
            {
              "title": "prev",
              "name": "prev",
              "type": "string",
              "default": null
            },
            {
              "title": "next",
              "name": "next",
              "type": "string",
              "default": null
            }
          ]
        }
      ]
    }
  ]
}